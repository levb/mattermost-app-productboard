{
	"app_id": "com.mattermost.product-board",
	"version": "0.0.8",
	"display_name": "ProductBoard integration",
	"app_type": "http",
	"root_url": "http://localhost:8080",
	"homepage_url": "https://github.com/levb/mattermost-app-productboard",
	"requested_permissions": [
		"act_as_admin",
		"act_as_bot",
		"act_as_user",
		"remote_oauth2",
		"remote_webhooks"
	],
	"requested_locations": [
		"/command",
		"/post_menu"
	],
	"bindings": {
		"path": "/bindings",
		"expand": {
			"acting_user": "all",
			"oauth2_user": "all"
		}
	}
}